{
  "name": "Next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start:testing-database": "cd backing-services && docker-compose up -d && cd ..",
    "stop:testing-database": "cd backing-services && docker-compose down && cd ..",
    "test": "npm run start:testing-database && dotenv -e .env.test -- prisma migrate deploy && jest && npm run stop:testing-database",
    "test:e2e": "cypress run --env SERVER_URL=http://localhost:3000,BFF_URL=http://localhost:3000/api",
    "test:open-e2e": "cypress open --env SERVER_URL=http://localhost:3000,BFF_URL=http://localhost:3000/api",
    "migrate:dev": "prisma db push",
    "migrate": "prisma migrate deploy",
    "open-sgbd": "prisma studio",
    "update-prisma-client": "prisma generate"
  },
  "dependencies": {
    "@next/env": "^12.0.3",
    "@prisma/client": "^3.4.0",
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^0.21.4",
    "dotenv-cli": "^4.1.0",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "babel-jest": "^27.2.5",
    "cypress": "^9.1.0",
    "dotenv": "^10.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "faker": "^5.5.3",
    "jest": "^27.2.5",
    "prisma": "^3.3.0",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.3.0",
    "typescript": "4.4.4",
    "uuid": "^8.3.2"
  }
}
